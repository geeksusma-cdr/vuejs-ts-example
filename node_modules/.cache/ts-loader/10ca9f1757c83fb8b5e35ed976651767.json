,KAAK,QAAQ,CAAA,EAAhB,wBAAgB;gBACV,EAAE,GAAG,MAAM,CAAC,GAAG,EAAE,KAAK,CAAC,CAAA;gBAC7B,qBAAM,KAAK,EAAE,EAAA;;gBAAb,SAAa,CAAA;gBACP,MAAM,GAAW;oBACnB,EAAE,EAAE,EAAE,CAAC,QAAQ,EAAE;oBACjB,QAAQ,EAAE,OAAO,CAAC,QAAQ;iBAC7B,CAAA;gBACD,sBAAO,OAAO,CAAC,OAAO,CAAC;wBACnB,IAAI,EAAE,MAAM;qBACf,CAAC,EAAA;;qBAGF,CAAA,GAAG,KAAK,UAAU,CAAA,EAAlB,wBAAkB;gBAClB,IAAI,OAAO,CAAC,QAAQ,KAAK,SAAS,IAAI,OAAO,CAAC,QAAQ,KAAK,QAAQ,EAAE;oBACjE,sBAAM;iBACT;gBAED,qBAAM,KAAK,EAAE,EAAA;;gBAAb,SAAa,CAAA;gBACP,MAAM,GAAW;oBACnB,EAAE,EAAE,GAAG;oBACP,QAAQ,EAAE,OAAO,CAAC,QAAQ;iBAC7B,CAAA;gBACD,sBAAO,OAAO,CAAC,OAAO,CAAC;wBACnB,IAAI,EAAE,MAAM;qBACf,CAAC,EAAA;;;;KAET,CAAA;AAED,aAAa;AACb,KAAK,CAAC,GAAG,GAAG,UAAO,GAAW,EAAE,OAAY;;;;;qBACpC,CAAA,GAAG,KAAK,QAAQ,CAAA,EAAhB,wBAAgB;gBAChB,qBAAM,KAAK,EAAE,EAAA;;gBAAb,SAAa,CAAA;gBACP,IAAI,yBACH,OAAO,KACV,KAAK,EAAE,OAAO,CAAC,KAAK,EACpB,OAAO,EAAE,OAAO,CAAC,OAAO,EACxB,QAAQ,EAAE,OAAO,CAAC,QAAQ,GAC7B,CAAA;gBACD,sBAAO,OAAO,CAAC,OAAO,CAAiB;wBACnC,IAAI,EAAE,IAAI;qBACb,CAAC,EAAA;;;;KAET,CAAA;AAGD,IAAM,GAAG,GAAG,SAAS,CAAC,GAAG,CAAC,CAAA;AAC1B,IAAM,MAAM,GAAG,eAAe,CAAC,KAAK,CAAC,CAAA;AACrC,GAAG,CAAC,GAAG,CAAC,MAAM,CAAC,CAAA;AACf,GAAG,CAAC,GAAG,CAAC,KAAK,CAAC,CAAA;AAEd,GAAG,CAAC,KAAK,CAAC,MAAM,CAAC,CAAA","sourcesContent":["import axios from 'axios'\nimport 'highlight.js/styles/atom-one-dark.css'\nimport random from 'lodash/random'\nimport { createApp } from 'vue'\nimport App from './App.vue'\nimport { demoData, Post, thisMonth, thisWeek, today } from './mocks'\nimport { routerWithStore } from './router'\nimport { Author, store } from './store'\n\nfunction delay() {\n    return new Promise(res => {\n        setTimeout(res, 500)\n    })\n}\n\n// @ts-ignore\naxios.get = async (url: string) => {\n    if (url === '/posts') {\n        await delay()\n        return Promise.resolve({\n            data: [today, thisWeek, thisMonth, ...demoData]\n        })\n    }\n}\n\n// @ts-ignore\naxios.post = async (url: string, payload: any) => {\n    if (url === '/posts') {\n        const id = random(100, 10000)\n        await delay()\n        const post: Post = {\n            ...payload,\n            id: id.toString(),\n            title: payload.title,\n            created: payload.created,\n            authorId: payload.authorId\n        }\n        return Promise.resolve<{ data: Post }>({\n            data: post\n        })\n    }\n\n    if (url === '/users') {\n        const id = random(100, 10000)\n        await delay()\n        const author: Author = {\n            id: id.toString(),\n            username: payload.username\n        }\n        return Promise.resolve({\n            data: author\n        })\n    }\n\n    if (url === '/sign_in') {\n        if (payload.username !== 'user123' || payload.password !== 'pass42') {\n            return\n        }\n\n        await delay()\n        const author: Author = {\n            id: '1',\n            username: payload.username\n        }\n        return Promise.resolve({\n            data: author\n        })\n    }\n}\n\n// @ts-ignore\naxios.put = async (url: string, payload: any) => {\n    if (url === '/posts') {\n        await delay()\n        const post: Post = {\n            ...payload,\n            title: payload.title,\n            created: payload.created,\n            authorId: payload.authorId\n        }\n        return Promise.resolve<{ data: Post }>({\n            data: post\n        })\n    }\n}\n\n\nconst app = createApp(App)\nconst router = routerWithStore(store)\napp.use(router)\napp.use(store)\n\napp.mount('#app')"]}]}