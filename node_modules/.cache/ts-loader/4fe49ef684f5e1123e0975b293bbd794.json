AK,CAAC,WAAW,GAAG,OAAO,CAAC,KAAK,CAAC;QACtD,CAAC,CAAC,CAAA;QACF,OAAO;YACH,KAAK,OAAA;YACL,OAAO,SAAA;YACP,eAAe,iBAAA;YACf,WAAW,aAAA;SACd,CAAA;IACL,CAAC;CACJ,CAAC,CAAC","sourcesContent":["\nimport { Post } from \"../mocks\";\nimport { defineComponent, onMounted, ref } from \"vue\"\n\nexport default defineComponent({\n\n    props: {\n        post: {\n            type: Object as () => Post,\n            required: true\n        }\n    },\n\n    setup(props) {\n        const title = ref(props.post.title);\n        const content = ref(\"## Title\\nEnter your post...\");\n        const contentEditable = ref<HTMLDivElement | null>(null);\n\n        const handleInput = () => {\n            if (!contentEditable.value) {\n                throw Error('This should never happen');\n            }\n            content.value = contentEditable.value.textContent || '';\n        }\n\n        onMounted(() => {\n            if (!contentEditable.value) {\n                throw Error('This should never happen');\n            }\n\n            contentEditable.value.textContent = content.value;\n        })\n        return {\n            title,\n            content,\n            contentEditable,\n            handleInput\n        }\n    }\n});\n"]}]}