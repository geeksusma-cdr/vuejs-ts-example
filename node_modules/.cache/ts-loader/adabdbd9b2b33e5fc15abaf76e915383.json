,QAAQ,EAAE,CAAA;QACxB,MAAM,KAAK,GAAG,QAAQ,EAAE,CAAA;QACxB,MAAM,MAAM,GAAG,KAAK,EAAE,GAAU,EAAE,EAAE;YAChC,IAAI,CAAC,cAAc,CAAC,KAAK,CAAC,KAAK;gBAC3B,CAAC,cAAc,CAAC,KAAK,CAAC,KAAK,EAC7B;gBACE,OAAM;aACT;YACD,MAAM,IAAI,GAAS;gBACf,EAAE,EAAE,IAAI;gBACR,QAAQ,EAAE,QAAQ,CAAC,KAAK;gBACxB,QAAQ,EAAE,QAAQ,CAAC,KAAK;aAC3B,CAAA;YACD,MAAM,KAAK,CAAC,MAAM,CAAC,IAAI,CAAC,CAAA;YACxB,KAAK,CAAC,SAAS,EAAE,CAAA;QACrB,CAAC,CAAA;QACD,OAAO;YACH,cAAc;YACd,QAAQ;YACR,MAAM;YACN,QAAQ;YACR,cAAc;SACjB,CAAA;IACL,CAAC;CACJ,CAAC,CAAC","sourcesContent":["\nimport { computed, defineComponent, ref } from 'vue';\nimport FormInput from './FormInput.vue'\nimport { required, length, Status, validate } from '../validation'\nimport { User, useStore } from '../store';\nimport { useModal } from '../useModal';\nexport default defineComponent({\n    components: {\n        FormInput\n    },\n    setup() {\n        const username = ref('')\n        const usernameStatus = computed<Status>(() => {\n            return validate(username.value, [required()])\n        })\n        const password = ref('')\n        const passwordStatus = computed<Status>(() => {\n            return validate(password.value, [required()])\n        })\n        const store = useStore()\n        const modal = useModal()\n        const submit = async (evt: Event) => {\n            if (!usernameStatus.value.valid ||\n                !passwordStatus.value.valid\n            ) {\n                return\n            }\n            const user: User = {\n                id: '-1',\n                username: username.value,\n                password: password.value,\n            }\n            await store.signIn(user)\n            modal.hideModal()\n        }\n        return {\n            usernameStatus,\n            username,\n            submit,\n            password,\n            passwordStatus\n        }\n    }\n});\n"]}]}