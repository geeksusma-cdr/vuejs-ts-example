K,EAAE,CAAC,QAAQ,EAAE,EAAE,MAAM,CAAC,EAAE,GAAG,EAAE,EAAE,EAAE,GAAG,EAAE,EAAE,EAAE,CAAC,CAAC,CAAC,CAAA;QAC7E,CAAC,CAAC,CAAA;QAEF,MAAM,KAAK,GAAG,QAAQ,EAAE,CAAA;QACxB,MAAM,KAAK,GAAG,QAAQ,EAAE,CAAA;QAExB,MAAM,MAAM,GAAG,KAAK,EAAE,GAAU,EAAE,EAAE;YAClC,IAAI,CAAC,cAAc,CAAC,KAAK,CAAC,KAAK;gBAC7B,CAAC,cAAc,CAAC,KAAK,CAAC,KAAK,EAC3B;gBACA,OAAM;aACP;YAED,MAAM,OAAO,GAAS;gBACpB,EAAE,EAAE,IAAI;gBACR,QAAQ,EAAE,QAAQ,CAAC,KAAK;gBACxB,QAAQ,EAAE,QAAQ,CAAC,KAAK;aACzB,CAAA;YAED,MAAM,KAAK,CAAC,UAAU,CAAC,OAAO,CAAC,CAAA;YAC/B,KAAK,CAAC,SAAS,EAAE,CAAA;QACnB,CAAC,CAAA;QAED,OAAO;YACL,cAAc;YACd,QAAQ;YACR,MAAM;YACN,QAAQ;YACR,cAAc;SACf,CAAA;IACH,CAAC;CACF,CAAC,CAAC","sourcesContent":["\nimport { computed, defineComponent, ref } from 'vue';\nimport FormInput from './FormInput.vue'\nimport { required, length, Status, validate } from '../validation'\nimport { User, useStore } from '../store';\nimport { useModal } from '../useModal';\n\nexport default defineComponent({\n  components: {\n    FormInput\n  },\n\n  setup() {\n    const username = ref('username')\n    const usernameStatus = computed<Status>(() => {\n      return validate(username.value, [required(), length({ min: 5, max: 10 })])\n    })\n\n    const password = ref('password')\n    const passwordStatus = computed<Status>(() => {\n      return validate(password.value, [required(), length({ min: 10, max: 40 })])\n    })\n\n    const store = useStore()\n    const modal = useModal()\n\n    const submit = async (evt: Event) => {\n      if (!usernameStatus.value.valid ||\n        !passwordStatus.value.valid\n      ) {\n        return\n      }\n\n      const newUser: User = {\n        id: '-1',\n        username: username.value,\n        password: password.value,\n      }\n\n      await store.createUser(newUser)\n      modal.hideModal()\n    }\n\n    return {\n      usernameStatus,\n      username,\n      submit,\n      password,\n      passwordStatus\n    }\n  }\n});\n"]}]}