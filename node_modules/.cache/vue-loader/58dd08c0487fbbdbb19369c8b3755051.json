C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;YACf,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;YACX,CAAC,CAAC,CAAC,CAAC;YACJ,CAAC,CAAC,CAAC;QACP;IACJ;AACJ,CAAC,CAAC","file":"/Users/geeksusma/vuejs-ts-example/src/components/PostWriter.vue","sourceRoot":"","sourcesContent":["<template>\n    <div class=\"columns\">\n        <div class=\"column\">\n            <div class=\"field\">\n                <div class=\"label\">New Post</div>\n                <input type=\"text\" class=\"input\" v-model=\"title\" />\n                <div class=\"column\">\n                    <div contenteditable ref=\"contentEditable\" @input=\"handleInput()\"></div>\n                </div>\n                <div class=\"column\">\n                    <div v-html=\"html\" />\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"columns\">\n        <div class=\"columns\">\n            <button @click=\"save()\" class=\"button is-primary\">Submit</button>\n        </div>\n    </div>\n</template>\n\n<script lang=\"ts\">\nimport { Post } from \"../mocks\";\nimport { defineComponent, onMounted, ref, watchEffect } from \"vue\"\nimport { parse } from \"marked\"\nimport highlight from \"highlight.js\"\n\nexport default defineComponent({\n\n    props: {\n        post: {\n            type: Object as () => Post,\n            required: true\n        }\n    },\n\n    emits: {\n        save: (post: Post) => {\n            return true;\n        }\n    },\n\n    setup(props, ctx) {\n        const title = ref(props.post.title);\n        const content = ref(\"## Title\\nEnter your post...\");\n        const html = ref(\"\");\n        const contentEditable = ref<HTMLDivElement | null>(null);\n\n        const handleInput = () => {\n            if (!contentEditable.value) {\n                throw Error('This should never happen');\n            }\n            content.value = contentEditable.value.innerText || '';\n        }\n\n        watchEffect(() => {\n            html.value = parse(content.value, {\n                gfm: true,\n                breaks: true,\n                highlight: (code: string) => {\n                    return highlight.highlightAuto(code).value\n                }\n            })\n        })\n\n        onMounted(() => {\n            if (!contentEditable.value) {\n                throw Error('This should never happen');\n            }\n\n            contentEditable.value.textContent = content.value;\n        })\n\n        const save = () => {\n            const newPost: Post = {\n                ...props.post,\n                title: title.value,\n                html: html.value,\n                markdown: content.value\n            }\n            //emit event\n            ctx.emit('save', newPost);\n\n        };\n        return {\n            title,\n            content,\n            contentEditable,\n            handleInput,\n            html,\n            save\n        }\n    }\n});\n</script>\n\n<style>\n.column {\n    overflow-y: scroll;\n}\n</style>"]}]}